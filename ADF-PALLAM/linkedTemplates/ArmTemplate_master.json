{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADF-PALLAM"
		},
		"AzureBlobStorage_EMP_SRC_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage_EMP_SRC'"
		},
		"LS_ABS_EVENT_TRIGER_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_ABS_EVENT_TRIGER'"
		},
		"SqlServer_EMP_TGT_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServer_EMP_TGT'"
		},
		"SqlServer_EMP_TGT_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SqlServer_EMP_TGT'"
		},
		"SqlServer_EMP_TGT_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Visitor"
		},
		"T_EventBased_properties_P_EVENT_TRIGGER_parameters_sourcePath": {
			"type": "string",
			"defaultValue": "@triggerBody().folderPath"
		},
		"T_EventBased_properties_P_EVENT_TRIGGER_parameters_sourceFile": {
			"type": "string",
			"defaultValue": "@triggerBody().fileName"
		},
		"T_EventBased_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/2599db6c-5530-4709-b593-47e788123f97/resourceGroups/MAHESH_PALLA/providers/Microsoft.Storage/storageAccounts/storgaemahesh"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADF-PALLAM_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage_EMP_SRC_connectionString": {
						"value": "[parameters('AzureBlobStorage_EMP_SRC_connectionString')]"
					},
					"LS_ABS_EVENT_TRIGER_connectionString": {
						"value": "[parameters('LS_ABS_EVENT_TRIGER_connectionString')]"
					},
					"SqlServer_EMP_TGT_connectionString": {
						"value": "[parameters('SqlServer_EMP_TGT_connectionString')]"
					},
					"SqlServer_EMP_TGT_password": {
						"value": "[parameters('SqlServer_EMP_TGT_password')]"
					},
					"SqlServer_EMP_TGT_properties_typeProperties_userName": {
						"value": "[parameters('SqlServer_EMP_TGT_properties_typeProperties_userName')]"
					},
					"T_EventBased_properties_P_EVENT_TRIGGER_parameters_sourcePath": {
						"value": "[parameters('T_EventBased_properties_P_EVENT_TRIGGER_parameters_sourcePath')]"
					},
					"T_EventBased_properties_P_EVENT_TRIGGER_parameters_sourceFile": {
						"value": "[parameters('T_EventBased_properties_P_EVENT_TRIGGER_parameters_sourceFile')]"
					},
					"T_EventBased_properties_typeProperties_scope": {
						"value": "[parameters('T_EventBased_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}